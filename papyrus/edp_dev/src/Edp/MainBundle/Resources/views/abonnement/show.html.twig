{% extends "EdpMainBundle::page_niv2.html.twig" %}

{% block body -%}
    <div class='row'>    
        <div class='col-sm-6 col-sm-offset-3' style="border:1px solid black; border-radius:10px;margin-top:20px">
    <h1 style="text-align:center">abonnement</h1>

    <table class="record_properties">
        <tbody>
            {#<tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>#}
            <tr>
                <th>Libelle</th>
                <td>{{ entity.libelle }}</td>
            </tr>
            <tr>
                <th>Debut</th>
                <td>{{ entity.debut|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Fin</th>
                <td>{{ entity.fin|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Isactive</th>
                <td>{{ entity.isActive }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('admin_abonnements') }}">
            Retour Ã  la liste des abonnements
        </a>
    </li>
    <li>
        <a href="{{ path('admin_abonnements_edit', { 'id': entity.id }) }}">
            Modifier
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
    
        </div>
        </div>
{% endblock %}
